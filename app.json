{
  "expo": {
    "name": "Froyo",
    "slug": "Froyo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/Logo-Background.png",
    "splash": {
      "image": "./assets/logo/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#41CA99"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/Logo-Background.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.protosapps.froyo"
    },
    "web": {
      "favicon": "./assets/logo/Logo-Background.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#707070"
   }
  }
}
